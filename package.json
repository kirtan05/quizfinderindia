    "express": "^5.2.1",
    "express-rate-limit": "^8.2.1",
    "helmet": "^8.1.0",
    "node-cron": "^4.2.1",
    "openai": "^6.22.0",
    "pino": "^10.3.1",
    "qrcode": "^1.5.4",
    "sanitize-html": "^2.17.1",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "zod": "^4.3.6"
  }
